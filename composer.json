{
    "name": "ericmann/totp",
    "description": "PHP5.6+ compatible implementation of the TOTP protocol.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Mann",
            "email": "eric@eamann.com",
            "homepage": "https://eamann.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "EAMann\\TOTP\\": "php/TOTP/"
        },
        "files": [
            "php/functions.php"
        ]
    },
    "require": {
        "php": ">=5.6.3",
        "paragonie/constant_time_encoding": "^1.0.0",
        "paragonie/random_compat": "^2.0"
    },
    "require-dev": {
        "doctrine/instantiator": ">=1.0, <1.1",
        "phpdocumentor/reflection-docblock": "<3.2.3",
        "phpunit/php-token-stream": "^1.4",
        "phpunit/phpunit": "^5.7",
        "satooshi/php-coveralls": "^1.0"
    }
}
