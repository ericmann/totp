{
    "name": "ericmann/totp",
    "description": "PHP implementation of the TOTP protocol.",
    "version": "2.0.0",
    "type": "library",
    "keywords": ["totp", "one-time password"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Mann",
            "email": "eric@eamann.com",
            "homepage": "https://eamann.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "EAMann\\TOTP\\": "php/TOTP/"
        },
        "files": [
            "php/functions.php"
        ]
    },
    "require": {
        "php": ">=7.4",
        "paragonie/constant_time_encoding": "^2.4"
    },
    "require-dev": {
		"phpunit/phpunit": "^8.5",
		"php-coveralls/php-coveralls": "^2.1",
		"squizlabs/php_codesniffer": "^3.4"
    }
}
